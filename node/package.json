{
  "name": "Movie-Database-API",
  "version": "1.0.0",
  "description": "App for rating movies",
  "main": "index.js",
  "scripts": {
    "server": "node -e \"require('./dist-server/app')\"",
    "clean": "rimraf dist-server",
    "transpile": "babel ./server --out-dir dist-server",
    "build": "npm-run-all clean transpile",
    "dev": "npm-run-all build server",
    "watch:dev": "nodemon",
    "seedDb": "node ./dist-server/scripts/seedDb.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*"
    ]
  },
  "author": "Zoe Heins",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "axios": "^0.19.2",
    "bcrypt": "<4.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "fetch": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.7",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
